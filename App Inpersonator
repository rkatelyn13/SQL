CREATE TABLE snapchat (id INTEGER PRIMARY KEY, 
        name TEXT, 
        birthday TEXT, 
        chat TEXT, 
        friend TEXT, 
        snaps NUMERIC);
        
INSERT INTO snapchat (name, birthday, chat, friend, snaps) VALUES ("Reyna", "1/13/97", "reynilla", "bobby", 105600);
